cc -g -o umake -Wall umake.c arg_parse.c target.c


ONE=${TWO}
A: B C
	echo target A
	echo Target A2
TWO=THREE
B: test2
	echo target B
	echo target B2
C: test3
	echo ${ONE}dqdwq${TWO} ${ONE}}festest
	echo target C2





umake: umake.o arg_parse.o target.o
	gcc -o umake -Wall umake.o arg_parse.o target.o
	echo Done
umake.o:
	gcc -c umake.c
arg_parse.o:
	gcc -c arg_parse.c
target.o:
	gcc -c target.c

